<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.sukanmi.neusportsman.pojo.User" table="users">
    <id column="id" name="id" type="long">
      <generator class="native"/>
    </id>
    <property column="email" name="email" type="string" unique="true"/>
    <property column="username" name="username" type="string"/>
    <property column="firstName" name="firstName" type="string"/>
    <property column="lastName" name="lastName" type="string"/>
    <property column="phoneNumber" name="phoneNumber" type="string"/>
    <property column="password" name="password" type="string"/>
  </class>
</hibernate-mapping>
