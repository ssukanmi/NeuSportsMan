<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.sukanmi.neusportsman.pojo.Event" table="events">
    <id column="id" name="id" type="long">
      <generator class="native"/>
    </id>
    <property column="owner" name="owner" type="string"/>
    <property column="title" name="title" type="string"/>
    <property column="host" name="host" type="string"/>
    <property column="venue" name="venue" type="string"/>
    <property column="description" name="description" type="string"/>
    <property column="dateAndTime" name="dateAndTime" type="string"/>
  </class>
</hibernate-mapping>
